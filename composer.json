{
  "name": "simgroep/broadway-simple-serializer",
  "description": "Simple serializer for Broadway",
  "license": "MIT",
  "require": {
    "php": ">=5.5.9",
    "broadway/broadway": "^1.0",
    "symfony/serializer": ">=2.4",
    "symfony/property-access": ">=2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
